<?xml version="1.0" encoding="UTF-8"?>
<!-- LEU compiler input file DTD  -->
<!-- $Id: LEU.dtd,v 1.1.2.2 2007/08/29 09:50:44 sebilleau Exp $-->
<!ELEMENT Application_Category (#PCDATA)>
<!ELEMENT Aspect (Mask, Telegram)>

<!ELEMENT Channel (#PCDATA)>
<!ELEMENT DATAVER (DATAVER_A, DATAVER_B, NUM_DATAVER, FSFB2_SRC_ADD)>
<!ATTLIST DATAVER
	id CDATA #REQUIRED
>
<!ELEMENT DATAVER_A (#PCDATA)>
<!ELEMENT DATAVER_B (#PCDATA)>
<!ELEMENT DELTA_N_MAX (#PCDATA)>
<!ELEMENT Default_telegram (#PCDATA)>
<!ELEMENT Encoder (TE_NUMBER, TEMPO_T1, TEMPO_T2, SYNC_MASTER_BOARD, NETWORK_GID, FIP_ENCODER_PN?, FIP_SUB_NETWORK_ADD?, SYSCKW_A, SYSCKW_B, SL_CHANGEOVER?)>
<!ATTLIST Encoder
	mode (Signalling | Transparent | Gateway) #REQUIRED
>
<!ELEMENT FIP_ENCODER_PN (#PCDATA)>
<!ELEMENT FIP_SUB_NETWORK_ADD (#PCDATA)>
<!ELEMENT FSFB2_Node_Cycle (#PCDATA)>
<!ELEMENT FSFB2_SRC_ADD (#PCDATA)>
<!ELEMENT FSFB2_Subnet_Ad (#PCDATA)>
<!ELEMENT GATEWAY (GATEWAY_IP_ADDRESS, GATEWAY_MAC_ADDRESS, GATEWAY_IP_PORT, IXL_IP_ADDRESS, IXL_IP_PORT, IXL_FSFB2_PARAMS+)>
<!ELEMENT GATEWAY_IP_ADDRESS (#PCDATA)>
<!ELEMENT GATEWAY_IP_PORT (#PCDATA)>
<!ELEMENT GATEWAY_MAC_ADDRESS (#PCDATA)>
<!ELEMENT GID (#PCDATA)>
<!ELEMENT INPUT_BOARD (SLOT, GID, TYPE)>
<!ATTLIST INPUT_BOARD
	id CDATA #REQUIRED
>
<!ELEMENT IXL_FSFB2_ADD (#PCDATA)>
<!ELEMENT IXL_FSFB2_PARAMS (IXL_FSFB2_ADD, IXL_VFIP_ADD)>
<!ELEMENT IXL_IP_ADDRESS (#PCDATA)>
<!ELEMENT IXL_IP_PORT (#PCDATA)>
<!ELEMENT IXL_VFIP_ADD (#PCDATA)>
<!ELEMENT Input (Channel, Number)>


<!ELEMENT LEU (INPUT_BOARD*, OUTPUT_BOARD?, SLIB_BOARD?, SLEB_BOARD?, Output_balise*, Encoder, Network, GATEWAY?, POSIXDATE_TPC, VERID_TPC, POSIXDATE_TSE, VERID_TSE, POSIXDATE_TE?, VERID_TE?)>
<!ATTLIST LEU
	name CDATA #IMPLIED
>
<!ELEMENT LIFETIME_MAX (#PCDATA)>
<!ELEMENT Mask (#PCDATA)>
<!ELEMENT Max_SSR_Retry (#PCDATA)>
<!ELEMENT Max_Tc_Err (#PCDATA)>
<!ELEMENT NETWORK_GID (#PCDATA)>
<!ELEMENT NUM_DATAVER (#PCDATA)>
<!ELEMENT N_BSD_TO_SSR (#PCDATA)>
<!ELEMENT Network (Transmit_Channel, Receive_Channel*)>
<!ELEMENT Number (#PCDATA)>
<!ELEMENT OUTPUT_BOARD (SLOT, GID, TYPE, TEL_number)>
<!ELEMENT Output_balise (Input*, Aspect*, Default_telegram)>
<!ATTLIST Output_balise
	id CDATA #REQUIRED
	telegram (SHORT | LONG) #REQUIRED
>
<!ELEMENT POSIXDATE_TE (#PCDATA)>
<!ELEMENT POSIXDATE_TPC (#PCDATA)>
<!ELEMENT POSIXDATE_TSE (#PCDATA)>
<!ELEMENT Receive_Channel (VFIP_ADD?, FSFB2_Node_Cycle, N_BSD_TO_SSR, FSFB2_SRC_ADD, Max_Tc_Err, Max_SSR_Retry, SID_A, SID_B, SINIT_A, SINIT_B, LIFETIME_MAX, DELTA_N_MAX, DATAVER_A, DATAVER_B, NUM_DATAVER)>
<!ATTLIST Receive_Channel
	id CDATA #REQUIRED
>
<!ELEMENT SID_A (#PCDATA)>
<!ELEMENT SID_B (#PCDATA)>
<!ELEMENT SINIT_A (#PCDATA)>
<!ELEMENT SINIT_B (#PCDATA)>
<!ELEMENT SLEB_BOARD (SLOT, GID, TYPE, SLEB_LINK_SPEED)>
<!ELEMENT SLEB_LINK_SPEED (#PCDATA)>
<!ELEMENT SLIB_BOARD (SLOT, GID, TYPE, SLIB_LINK)>
<!ELEMENT SLIB_LINK (#PCDATA)>
<!ATTLIST SLIB_LINK
	id CDATA #REQUIRED
>
<!ELEMENT SLOT (#PCDATA)>
<!ELEMENT SL_CHANGEOVER (#PCDATA)>
<!ELEMENT SYNC_MASTER_BOARD (#PCDATA)>
<!ELEMENT SYSCKW_A (#PCDATA)>
<!ELEMENT SYSCKW_B (#PCDATA)>
<!ELEMENT TEL_number (#PCDATA)>
<!ELEMENT TEMPO_T1 (#PCDATA)>
<!ELEMENT TEMPO_T2 (#PCDATA)>
<!ELEMENT TE_NUMBER (#PCDATA)>
<!ELEMENT TYPE (#PCDATA)>
<!ELEMENT Telegram (#PCDATA)>
<!ELEMENT Transmit_Channel (VFIP_ADD?, Application_Category, FSFB2_Subnet_Ad, FSFB2_SRC_ADD, SID_A, SID_B, SINIT_A, SINIT_B, DATAVER*)>
<!ELEMENT VERID_TE (#PCDATA)>
<!ELEMENT VERID_TPC (#PCDATA)>
<!ELEMENT VERID_TSE (#PCDATA)>
<!ELEMENT VFIP_ADD (#PCDATA)>
